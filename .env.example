# Google AI Studio API Key (for using Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Use Vertex AI instead of Google AI Studio (optional)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Google Cloud Project ID (required if using Vertex AI)
GOOGLE_CLOUD_PROJECT=your_project_id_here

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# CORS allowlist for web frontend (comma-separated)
CORS_ALLOW_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
# Optional: allow local/LAN via regex (Python style)
# Example matches localhost, 10.x, 192.168.x, 100.64.x (CGNAT), any port
# CORS_ALLOW_ORIGIN_REGEX=^https?://(localhost|127\.0\.0\.1|10\.\d+\.\d+\.\d+|192\.168\.\d+\.\d+|100\.64\.\d+\.\d+)(:\\d+)?$

# Next.js dev server: allowed cross-origin dev access (comma separated)
# ALLOWED_DEV_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# FastAPI port for make dev-api (optional)
API_PORT=8000
# API base settings
# Logging
HIBIKASU_LOG_LEVEL=INFO
# Set to 1 to include stack traces for ADK failures
HIBIKASU_LOG_TRACE=

# ADK (Gemini) integration
# Set to 1 to disable ADK initialization (use local runtime impl only)
HIBIKASU_DISABLE_ADK=
# Model name for ADK agents
ADK_MODEL=gemini-2.5-flash-lite
# Timeout (seconds) for ADK review execution
ADK_REVIEW_TIMEOUT_SEC=40
